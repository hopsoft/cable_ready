{
  "name": "cable_ready",
  "version": "4.4.0-pre2",
  "description": "CableReady helps you create great real-time user experiences by making it simple to trigger client-side DOM changes from server-side Ruby.",
  "keywords": [
    "ruby",
    "rails",
    "websockets",
    "actioncable",
    "cable",
    "ssr",
    "stimulus_reflex",
    "client-side",
    "dom"
  ],
  "repository": "https://github.com/hopsoft/cable_ready",
  "homepage": "https://cableready.stimulusreflex.com/",
  "bugs": "https://github.com/hopsoft/cable_ready/issues",
  "license": "MIT",
  "author": "Nathan Hopkins <natehop@gmail.com>",
  "source": "index.js",
  "main": "dist/cable_ready.js",
  "module": "dist/cable_ready.module.js",
  "esmodule": "dist/cable_ready.modern.js",
  "scripts": {
    "prepare": "yarn build",
    "prettier-standard:check": "yarn run prettier-standard --check *.js **/*.js",
    "prettier-standard:format": "yarn run prettier-standard *.js **/*.js",
    "build": "microbundle --target browser --format modern,es,cjs --no-strict",
    "dev": "microbundle watch --target browser --format modern,es,cjs --no-strict",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@cable_ready/core": "4.4.0-pre2"
  },
  "devDependencies": {
    "microbundle": "^0.12.4",
    "prettier-standard": "^16.4.1",
    "rimraf": "^3.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
